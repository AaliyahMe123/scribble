<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label "Enter Post Content" %>
  <%= f.text_area :body %>

  <%= f.label "Post Image URL" %>
  <%= f.text_field :img_url %>

  <%= f.submit "Create Post" %>
<% end %>
